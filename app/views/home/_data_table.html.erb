<table class="table">
  <tr>
    <th>Building</th>
    <th>Year</th>
    <%- @presenter.each_measurement do |measurement|%>
        <th><%= measurement%></th>
    <% end %>
  </tr>

  <%- @presenter.each_building_attrs do |name, year, attrs|%>
      <tr>
        <td><%= name%></td>
        <td><%= year%></td>
        <%- attrs.each do |attr|%>
            <td><%= attr%></td>
        <% end %>
      </tr>
  <% end %>
</table>